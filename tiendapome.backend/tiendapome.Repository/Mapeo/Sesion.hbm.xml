<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping assembly="tiendapome.Entidades" namespace="tiendapome.Entidades" xmlns="urn:nhibernate-mapping-2.2">
    <class name="tiendapome.Entidades.Sesion" table="tp_sesiones" lazy="false">
        <id name="Id" column="IdSesion">
            <generator class="identity" />
        </id>
        <property name="IdCliente" column="IdCliente" />
        <property name="Token" column="Token" />
        <property name="Device" column="Device" />
    </class>
</hibernate-mapping>